{% extends 'base.html.twig' %}

{% block title %}TeamEmployee index{% endblock %}

{% block body %}
    <h1>TeamEmployee index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for team_employee in team_employees %}
            <tr>
                <td>{{ team_employee.id }}</td>
                <td>
                    <a href="{{ path('app_team_employee_show', {'id': team_employee.id}) }}">show</a>
                    <a href="{{ path('app_team_employee_edit', {'id': team_employee.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_team_employee_new') }}">Create new</a>
{% endblock %}
